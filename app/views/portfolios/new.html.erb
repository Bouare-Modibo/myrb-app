<h1>Create a new Portfolio Items</h1>


<%= form_with(model: @single_pf) do |form| %>

  <div>
    <%= form.label :tittle, style: "display: block" %>
    <%= form.text_field :tittle %>
  </div>

  <div>
    <%= form.label :subtitle, style: "display: block" %>
    <%= form.text_field :subtitle %>
  </div>

  <div>
    <%= form.label :body, style: "display: block" %>
    <%= form.text_area :body %>
  </div>

  <div>
    <%= form.submit %>
  </div>
  
<% end %>